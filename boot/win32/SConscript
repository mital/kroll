#!/usr/bin/env python
Import('build')

Env = build.env.Clone()
Env.Append(CPPDEFINES={'WIN32_LEAN_AND_MEAN': 1})
Env.Append(CPPPATH=['#common/win32','#common'])
Env.Append(LIBS = ['shell32', 'user32'])

Env.Program(build.dir + '/kboot', ['boot.cpp','../api/file_utils.obj', '../api/unzip/unzip.obj'])
